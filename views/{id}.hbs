{{!
{
	"title": "Killer Photo Album",
	"name": "album", 
	"description": "Nice photo album.",
	"albumid": "{id}",
	"resources": {
		"albuminfo": "https://graph.facebook.com/{id}?fields=id,count,cover_photo,created_time,name",
		"album_photos": "https://graph.facebook.com/{id}/photos"
	}
}
}}
<div class="row album_photos">
	<div class="col-md-4 info">
		<img class="img-responsive logo" src="/images/logo.png">
		<div class="itemInfo">
			<h3>{{resources.albuminfo.name}}</h3>
			<p>Album created on:{{formatDate resources.albuminfo.created_time "%B %o, %Y"}}</p>	
			<a href="/" class="linkback">Back to albums list</a>
		</div>
	</div>
	<div id="mainImage" class="col-md-8">
		<img src="https://graph.facebook.com/{{resources.albuminfo.cover_photo}}/picture?type=normal" class="img-responsive"/>
	</div>
</div>
{{> partials/thumbnails }}